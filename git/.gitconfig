[github]
  # user = atlsapi
[color]
  ui = auto
[alias]
  lg = log
  st = status
  co = checkout
  cob = checkout -b
  ci = commit
  pl = pull
  plr = pull --rebase
  df = diff
  df = diff --word-diff
  pm = push origin master
  pd = push origin dev
  fep = fetch --prune
  ri = rebase -i
  brad = branch -D 
  lol = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	 

[merge]
	tool = kdiff3
[mergetool "kdiff3"]
	path = C:/Program Files/KDiff3/kdiff3.exe
	trustExitCode = false
